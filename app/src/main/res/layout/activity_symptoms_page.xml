<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".symptomsPage">

    <TextView
        android:id="@+id/textView10"
        android:layout_width="353dp"
        android:layout_height="47dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="57dp"
        android:text="Tap on a symptom to learn more"
        android:textColor="@android:color/black"
        android:textSize="24sp"
        android:layout_marginBottom="20dp" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="296dp"
        android:layout_height="30dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="179dp"
        android:text="Erratic changes in mood"
        android:layout_marginBottom="15dp" />

    <Button
        android:id="@+id/moodswings"
        android:layout_width="180dp"
        android:layout_height="42dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="128dp"
        android:onClick="toMoodSwings"
        android:text="Mood swings"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="75dp" />
    <Button
        android:id="@+id/impulse"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="131dp"
        android:onClick="toImpulse"
        android:text="Impulsive Actions"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="318dp"/>


    <TextView
        android:id="@+id/textView14"
        android:layout_width="297dp"
        android:layout_height="24dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="199dp"
        android:onClick="toMoodSwings"
        android:text="Tasks are performed less efficienctly"
        tools:layout_editor_absoluteX="27dp"
        tools:layout_editor_absoluteY="257dp" />

    <Button
        android:id="@+id/productivity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:onClick="toDecInProd"
        android:text="Decrease in Productivity"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="195dp"/>

    <TextView
        android:id="@+id/textView15"
        android:layout_width="302dp"
        android:layout_height="47dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="71dp"
        android:text="Tendency to make decisions without thinking about the consequences"
        tools:layout_editor_absoluteX="27dp"
        tools:layout_editor_absoluteY="378dp" />
</RelativeLayout>

